DA,yBACA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBAEA,KAAA,CAAA;AACA,kBAAA,MAAA,EAAA,KADA;AAEA,kBAAA,GAAA,0CAAA,KAAA,CAAA,SAAA,CAFA;AAGA,kBAAA,OAAA,EAAA;AACA,oBAAA,aAAA,EAAA,YAAA,KAAA,CAAA;AADA;AAHA,iBAAA,CAFA;;AAAA;AAEA,gBAAA,QAFA;AASA,gBAAA,KAAA,CAAA,QAAA,GAAA,QAAA,CAAA,IAAA;;AACA,gBAAA,KAAA,CAAA,QAAA,CAAA,OAAA,CAAA,UAAA,GAAA,EAAA,KAAA,EAAA;AACA,kBAAA,KAAA,CAAA,MAAA,CAAA,IAAA,CAAA,KAAA,CAAA,kBAAA,CAAA,GAAA,CAAA;AACA,iBAFA;;AAVA;AAAA;;AAAA;AAAA;AAAA;AAcA,gBAAA,IAAA,CAAA,IAAA,CAAA;AACA,kBAAA,IAAA,EAAA,OADA;AAEA,kBAAA,KAAA;AAFA,iBAAA;AAIA,gBAAA,QAAA,CAAA,WAAA;;AAlBA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAoBA,KArBA;AAuBA,IAAA,wBAvBA,oCAuBA,OAvBA,EAuBA;AACA,aAAA,OAAA,CAAA,SAAA,CAAA,CAAA,EAAA,OAAA,CAAA,OAAA,CAAA,GAAA,CAAA,CAAA;AACA,KAzBA;AA2BA,IAAA,kBA3BA,8BA2BA,OA3BA,EA2BA;AACA,UAAA,OAAA,CAAA,UAAA,IAAA,IAAA,EAAA;AACA,YAAA,GAAA,GAAA,OAAA,CAAA,UAAA,CAAA,KAAA,CAAA,QAAA,EAAA,CAAA,CAAA;AACA,QAAA,GAAA,GAAA,kBAAA,CAAA,GAAA,CAAA,OAAA,CAAA,KAAA,EAAA,GAAA,CAAA,CAAA;AACA,QAAA,GAAA,GAAA,GAAA,CAAA,SAAA,CAAA,CAAA,EAAA,CAAA,IAAA,GAAA,GAAA,GAAA,CAAA,SAAA,CAAA,CAAA,EAAA,GAAA,CAAA,MAAA,CAAA;AACA,eAAA,GAAA;AACA;AACA,KAlCA;AAmCA,IAAA,cAnCA,4BAmCA;AACA,UAAA,IAAA,GAAA,QAAA,CAAA,GAAA;;AACA,UAAA,IAAA,CAAA,OAAA,CAAA,GAAA,IAAA,CAAA,CAAA,EAAA;AACA,aAAA,SAAA,IAAA,IAAA,CAAA,SAAA,CAAA,IAAA,CAAA,OAAA,CAAA,GAAA,CAAA,CAAA;AACA;AACA;;AACA,WAAA,SAAA,IAAA,GAAA;AACA,KA1CA;AA2CA,IAAA,eA3CA,2BA2CA,WA3CA,EA2CA;AACA,UAAA,GAAA,GAAA,QAAA,CAAA,aAAA,CAAA,KAAA,CAAA;AACA,MAAA,GAAA,CAAA,SAAA,GAAA,WAAA;AACA,UAAA,IAAA,GAAA,GAAA,CAAA,WAAA,IAAA,GAAA,CAAA,SAAA,IAAA,EAAA;AACA,aAAA,IAAA,CAAA,KAAA,CAAA,GAAA,EAAA,CAAA,CAAA;AACA;AAhDA,GAjBA;AAmEA,EAAA,QAAA,EAAA;AACA,IAAA,IADA,kBACA;AACA,aAAA,KAAA,QAAA,CAAA,MAAA;AACA;AAHA;AAnEA,CAAA","sourcesContent":["<template>\r\n  <body class=\"no-margin\">\r\n    <nav\r\n      class=\"navbar navbar-expand-lg navbar-light\"\r\n      style=\"background-color: black\"\r\n    >\r\n      <img\r\n        src=\"../assets/logo.png\"\r\n        alt=\"Background\"\r\n        height=\"100px\"\r\n        width=\"100px\"\r\n        id=\"imageLogo\"\r\n      />\r\n\r\n      <div class=\"collapse navbar-collapse\" id=\"conteudoNavbarSuportado\">\r\n        <ul class=\"navbar-nav mr-auto\">\r\n          <li class=\"nav-item active\">\r\n            <router-link class=\"nav-link text-white\" :to=\"{ name: 'Home' }\"\r\n              >Início</router-link\r\n            >\r\n          </li>\r\n          <li class=\"nav-item\">\r\n            <router-link class=\"nav-link text-white\" :to=\"{ name: 'Home' }\"\r\n              >Calistenia</router-link\r\n            >\r\n          </li>\r\n          <li class=\"nav-item\">\r\n            <router-link class=\"nav-link text-white\" :to=\"{ name: 'Articles' }\"\r\n              >Artigos</router-link\r\n            >\r\n          </li>\r\n          <li class=\"nav-item\">\r\n            <router-link class=\"nav-link text-white\" :to=\"{ name: 'About' }\"\r\n              >Contato</router-link\r\n            >\r\n          </li>\r\n        </ul>\r\n      </div>\r\n    </nav>\r\n\r\n    <form>\r\n      <div\r\n        class=\"form-group container-fluid d-flex flex-column align-items-start wide\"\r\n      >\r\n        <label for=\"search\" class=\"mt-2\">Procure por artigos: </label>\r\n        <input\r\n          type=\"search\"\r\n          name=\"searcher\"\r\n          ref=\"searcher\"\r\n          class=\"form-control w-100\"\r\n          id=\"search\"\r\n          placeholder=\"Digite aqui\"\r\n        />\r\n      </div>\r\n    </form>\r\n\r\n    <div class=\"container-fluid\">\r\n      <ul\r\n        class=\"d-flex justify-content-between mr-5 ml-3\"\r\n        style=\"list-style: none\"\r\n      >\r\n        <li\r\n          v-for=\"(art, index) in articles.slice(\r\n            (currentPage - 1) * perPage,\r\n            (currentPage - 1) * perPage + perPage\r\n          )\"\r\n          v-bind:key=\"art.id\"\r\n          class=\"mt-5\"\r\n        >\r\n          \r\n          <div class=\"card\" style=\"max-width: 20vw\">\r\n            <img\r\n              class=\"card-img-top\"\r\n              :src=\"images[index]\"\r\n              id=\"articleImg\"\r\n              alt=\"Imagem do artigo\"\r\n              v-if=\"art.image_post\"\r\n            />\r\n            <div class=\"card-body\">\r\n              <div class=\"card-text article-item\">\r\n                <router-link\r\n                  style=\"text-decoration: none\"\r\n                  :to=\"{\r\n                    name: 'Article',\r\n                    params: { id: art.id, title: art.title },\r\n                  }\"\r\n                >\r\n                  {{ art.title }}\r\n                  <p class=\"gallery\">\r\n                    {{ writeSummernote(art.text) }}\r\n                  </p>\r\n                  <span class=\"gallery\"\r\n                    >{{ art.category }} | Leonard | 12/12/2021 às 10:30</span\r\n                  >\r\n                </router-link>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </li>\r\n      </ul>\r\n\r\n      <!-- <h2 class=\"display-3 text-center\" >Não foram encontrados artigos com esse termo.</h2> -->\r\n\r\n      <b-pagination\r\n        class=\"d-flex justify-content-center align-items-end mt-5\"\r\n        v-model=\"currentPage\"\r\n        :total-rows=\"rows\"\r\n        :per-page=\"perPage\"\r\n        aria-controls=\"articleImg\"\r\n      ></b-pagination>\r\n    </div>\r\n  </body>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\n\r\nconst firebase = require(\"../components/app.js\");\r\nconst errorLog = require(\"../components/backend_errors.js\");\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      articles: [],\r\n      images: [],\r\n      token: sessionStorage.access,\r\n      perPage: 4,\r\n      currentPage: 1,\r\n      searchKey: ''\r\n    };\r\n  },\r\n  created() {\r\n    this.getSearchedKey();\r\n  },\r\n  mounted() {\r\n    this.getArticles();\r\n  },\r\n  methods: {\r\n    async getArticles() {\r\n      try {\r\n        let response = await axios({\r\n          method: \"get\",\r\n          url: `http://localhost:8000/articles${this.searchKey}`,\r\n          headers: {\r\n            Authorization: \"Bearer \" + this.token,\r\n          },\r\n        });\r\n        this.articles = response.data;\r\n        this.articles.forEach((art, index) => {\r\n          this.images.push(this.getArticleImageURL(art));\r\n        });\r\n      } catch (e) {\r\n        swal.fire({\r\n          icon: 'error',\r\n          title: `Erro: ${e}`\r\n        })\r\n        errorLog.errorHandle(e);\r\n      }\r\n    },\r\n\r\n    getArticleFirstParagraph(article) {\r\n      return article.substring(0, article.indexOf(\".\"));\r\n    },\r\n\r\n    getArticleImageURL(article) {\r\n      if (article.image_post != null) {\r\n        let url = article.image_post.split('media/')[1]\r\n        url = decodeURIComponent(url.replace(/\\+/g,  \" \"))\r\n        url = url.substring(0, 6) + '/' + url.substring(6, url.length)\r\n        return url\r\n      }\r\n    },\r\n    getSearchedKey() {\r\n      const path = document.URL;\r\n      if (path.indexOf('?') > -1) {\r\n        this.searchKey += path.substring(path.indexOf('?'));\r\n        return\r\n      }\r\n      this.searchKey += '/';\r\n    },\r\n    writeSummernote(articleText) {\r\n      var div = document.createElement(\"div\");\r\n      div.innerHTML = articleText;\r\n      var text = div.textContent || div.innerText || \"\";\r\n      return text.split('.')[0];\r\n    }\r\n  },\r\n  computed: {\r\n    rows() {\r\n      return this.articles.length;\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n@import url(\"../style.css\");\r\n</style>\r\n"],"sourceRoot":"src/views"}]}