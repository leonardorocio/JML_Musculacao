,OAAA,EAAA,QAAA;AAAA;;AAHA;AAAA;AAAA,uBAKA,KAAA,CAAA;AACA,kBAAA,MAAA,EAAA,OADA;AAEA,kBAAA,GAAA,2CAAA,QAAA,CAAA,MAAA,CAAA,QAAA,CAAA,MAFA;AAGA,kBAAA,OAAA,EAAA;AACA,oBAAA,aAAA,EAAA,YAAA,MAAA,CAAA;AADA,mBAHA;AAMA,kBAAA,IAAA,EAAA;AANA,iBAAA,CALA;;AAAA;AAKA,gBAAA,QALA;AAaA,gBAAA,IAAA,CACA,IADA,CACA;AACA,kBAAA,IAAA,EAAA,SADA;AAEA,kBAAA,KAAA;AAFA,iBADA,EAKA,IALA,CAKA,UAAA,MAAA,EAAA;AACA,kBAAA,MAAA,CAAA,QAAA,CAAA,MAAA;AACA,iBAPA;AAbA;AAAA;;AAAA;AAAA;AAAA;AAsBA,gBAAA,QAAA,CAAA,WAAA;;AAtBA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAwBA;AA/FA;AAhBA,CAAA","sourcesContent":["<template>\r\n  <body id=\"fundoCadastro\">\r\n    <div\r\n      class=\"container bg-white profiles d-flex flex-column justify-content-around\"\r\n    >\r\n      <h2 class=\"display-4 text-center\" id=\"meuPerfil\">Meu Perfil</h2>\r\n\r\n      <form action=\"\" id=\"profileForm\" ref=\"profileForm\">\r\n        <div class=\"container-fluid d-flex align-items-center flex-column\">\r\n          <img\r\n            :src=\"image\"\r\n            alt=\"Icone Rosto\"\r\n            id=\"faceIcon\"\r\n            @click=\"chooseImage\"\r\n          />\r\n          <img src=\"\" id=\"toUpload\" style=\"display: none\" alt=\"\" />\r\n          <div class=\"form-group d-flex align-items-center flex-column\">\r\n            <label for=\"faceInput\" class=\"\" id=\"userInfo\">Perfil</label>\r\n            <input\r\n              type=\"file\"\r\n              ref=\"fileInput\"\r\n              class=\"form-control-file form-control-sm\"\r\n              id=\"faceInput\"\r\n              name=\"faceInput\"\r\n              @input=\"onSelectFile\"\r\n            />\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"form-group row mb-2\">\r\n          <label for=\"age\" class=\"col-sm-2\">Idade:</label>\r\n          <div class=\"col-sm-10\">\r\n            <input\r\n              placeholder=\"Ex: 25\"\r\n              type=\"text\"\r\n              class=\"form-control form-control-sm\"\r\n              id=\"age\"\r\n              name=\"age\"\r\n              :value=\"age\"\r\n            />\r\n          </div>\r\n        </div>\r\n        <div class=\"form-group row mb-2\">\r\n          <label for=\"altura\" class=\"col-sm-2\">Altura (em Metros):</label>\r\n          <div class=\"col-sm-10\">\r\n            <input\r\n              placeholder=\"Ex: 1.70\"\r\n              type=\"text\"\r\n              class=\"form-control form-control-sm\"\r\n              id=\"height\"\r\n              name=\"height\"\r\n              :value=\"height\"\r\n            />\r\n          </div>\r\n        </div>\r\n        <div class=\"form-group row mb-2\">\r\n          <label for=\"peso\" class=\"col-sm-2\">Peso (em KG):</label>\r\n          <div class=\"col-sm-10\">\r\n            <input\r\n              placeholder=\"Ex: 65.5\"\r\n              type=\"text\"\r\n              class=\"form-control form-control-sm\"\r\n              id=\"weight\"\r\n              name=\"weight\"\r\n              :value=\"weight\"\r\n            />\r\n          </div>\r\n        </div>\r\n        <div class=\"form-check form-check-inline\">\r\n          <label class=\"form-check-label mr-5\" for=\"\">Sexo Biológico: </label>\r\n          <input\r\n            class=\"form-check-input ml-5\"\r\n            type=\"radio\"\r\n            name=\"biosex\"\r\n            id=\"sexFeminino\"\r\n            value=\"F\"\r\n            :checked=\"biosex == 'F' ? true : false\"\r\n          />\r\n          <label class=\"form-check-label\" for=\"sexFeminino\">Feminino</label>\r\n        </div>\r\n        <div class=\"form-check form-check-inline\">\r\n          <input\r\n            class=\"form-check-input\"\r\n            type=\"radio\"\r\n            name=\"biosex\"\r\n            id=\"sexMasculino\"\r\n            value=\"M\"\r\n            :checked=\"biosex == 'M' ? true : false\"\r\n          />\r\n          <label class=\"form-check-label\" for=\"sexMasculino\">Masculino</label>\r\n        </div>\r\n      </form>\r\n      <button\r\n        type=\"button\"\r\n        class=\"btn btn-primary wid p-2 mb-1\"\r\n        @click=\"uploadToProfile()\"\r\n      >\r\n        Salvar Perfil\r\n      </button>\r\n    </div>\r\n  </body>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\n\r\nconst firebase = require(\"../components/app.js\");\r\nconst errorLog = require(\"../components/backend_errors.js\");\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      owner_id: sessionStorage.id,\r\n      age: undefined,\r\n      height: undefined,\r\n      weight: undefined,\r\n      biosex: \"\",\r\n      token: sessionStorage.access,\r\n      image: require(\"../assets/icone_rosto.png\"),\r\n    };\r\n  },\r\n  async created() {\r\n    this.getUserProfile();\r\n  },\r\n\r\n  methods: {\r\n    uploadToProfile() {\r\n      const facePath = document.getElementById(\"faceInput\").files[0];\r\n      if (facePath != undefined) {\r\n        const imagesRef = firebase.storRef.child(\"images/\" + facePath.name);\r\n        const uploader = this;\r\n\r\n        imagesRef\r\n          .put(facePath)\r\n          .then((snapshot) => {\r\n            firebase.storRef\r\n              .child(imagesRef.fullPath)\r\n              .getDownloadURL()\r\n              .then((url) => {\r\n                uploader.saveProfile(url);\r\n              });\r\n          })\r\n          .catch(function (error) {\r\n            swal.fire({\r\n              icon: \"error\",\r\n              title: \"Erro não foi possível salvar a imagem.\",\r\n            });\r\n          });\r\n      } else {\r\n        this.saveProfile(null);\r\n      }\r\n    },\r\n\r\n    chooseImage() {\r\n      this.$refs.fileInput.click();\r\n    },\r\n\r\n    onSelectFile() {\r\n      const input = this.$refs.fileInput;\r\n      const files = input.files;\r\n      if (files && files[0]) {\r\n        const reader = new FileReader();\r\n\r\n        reader.onload = (e) => {\r\n          this.$emit('image', e.target.result)\r\n          this.image = e.target.result;\r\n        };\r\n\r\n        reader.readAsDataURL(files[0]);\r\n      }\r\n    },\r\n\r\n    async getUserProfile() {\r\n      try {\r\n        let response = await axios({\r\n          method: \"GET\",\r\n          url: `http://localhost:8000/profiles/${parseInt(this.owner_id)}/`,\r\n          headers: {\r\n            Authorization: `Bearer ${this.token}`,\r\n          },\r\n        });\r\n        let resp = response.data;\r\n        const obj = this\r\n        for (let dt in resp) {\r\n          if (\r\n            dt != \"owner_id\" &&\r\n            dt != 'token'\r\n          ) {\r\n            obj.$data[dt] = resp[dt];\r\n          }\r\n        }\r\n      } catch (e) {\r\n        errorLog.errorHandle(e)\r\n      }\r\n    },\r\n\r\n    async saveProfile(filepath) {\r\n      const form = this.$refs.profileForm;\r\n      var formData = new FormData(form);\r\n      if (filepath != null) { formData.append('image', filepath) }\r\n      try {\r\n        let response = await axios({\r\n          method: \"PATCH\",\r\n          url: `http://localhost:8000/profiles/${parseInt(this.owner_id)}/`,\r\n          headers: {\r\n            Authorization: \"Bearer \" + this.token,\r\n          },\r\n          data: formData, \r\n        });\r\n        swal\r\n          .fire({\r\n            icon: \"success\",\r\n            title: `Usuário atualizado com sucesso`,\r\n          })\r\n          .then((result) => {\r\n            window.location.reload();\r\n          });\r\n      } catch (e) {\r\n        errorLog.errorHandle(e);\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style></style>\r\n"],"sourceRoot":"src/views"}]}