AA,CADA;;AAAA;AACA,gBAAA,OADA;AAQA,gBAAA,KAAA,CAAA,QAAA,GAAA,OAAA,CAAA,IAAA;;AARA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AASA,KAVA;AAWA,IAAA,YAXA,wBAWA,IAXA,EAWA;AACA,MAAA,IAAA,GAAA,IAAA,CACA,SADA,CACA,KADA,EAEA,OAFA,CAEA,mCAFA,EAEA,EAFA,CAAA;AAGA,aAAA,IAAA,CAAA,UAAA,CAAA,GAAA,EAAA,GAAA,EAAA,WAAA,EAAA;AACA,KAhBA;AAiBA,IAAA,eAjBA,6BAiBA;AACA,UAAA,UAAA,GAAA,KAAA,KAAA,CAAA,UAAA,CAAA,KAAA;AACA,WAAA,eAAA,CAAA,IAAA,CAAA,UAAA;AACA,MAAA,UAAA,GAAA,EAAA;AACA,KArBA;AAsBA,IAAA,iBAtBA,6BAsBA,KAtBA,EAsBA;AACA,WAAA,YAAA,CAAA,MAAA,CAAA,KAAA,EAAA,CAAA;AACA,KAxBA;AAyBA,IAAA,eAzBA,6BAyBA;AACA,UAAA,WAAA,GAAA,EAAA;AACA,WAAA,eAAA,CAAA,OAAA,CAAA,UAAA,EAAA,EAAA,KAAA,EAAA;AACA,QAAA,WAAA,CAAA,KAAA,CAAA,QAAA,EAAA,CAAA,GAAA,EAAA;AACA,OAFA;AAGA,MAAA,KAAA,CAAA;AACA,QAAA,MAAA,EAAA,OADA;AAEA,QAAA,GAAA,wCAAA,cAAA,CAAA,EAAA,CAAA,QAAA,EAAA,MAFA;AAGA,QAAA,IAAA,EAAA;AACA,UAAA,OAAA,EAAA;AADA,SAHA;AAMA,QAAA,OAAA,EAAA;AACA,UAAA,aAAA,mBAAA,cAAA,CAAA,MAAA;AADA;AANA,OAAA,CAAA,CAUA,IAVA,CAUA,YAAA;AACA,QAAA,IAAA,CAAA,IAAA,CAAA;AACA,UAAA,IAAA,EAAA,SADA;AAEA,UAAA,KAAA,EAAA;AAFA,SAAA;AAIA,OAfA,EAgBA,KAhBA,CAgBA,UAAA,CAAA,EAAA;AACA,QAAA,IAAA,CAAA,IAAA,CAAA;AACA,UAAA,IAAA,EAAA,OADA;AAEA,UAAA,KAAA,kBAAA,CAAA;AAFA,SAAA;AAIA,OArBA;AAsBA;AApDA,GAZA;AAkEA,EAAA,QAAA,EAAA;AACA,IAAA,eADA,6BACA;AACA,aAAA,cAAA,CAAA,OAAA,CAAA,KAAA,CAAA,GAAA,CAAA;AACA;AAHA;AAlEA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <div v-if=\"userWorkouts.length < 3 && userWorkouts\">\r\n      <p>Adicione treinos ao seu perfil para melhorar sua organização!</p>\r\n      <p>Para adicionar um treino, basta colocar o nome dele na caixa de seleção abaixo e confirmar com o botao</p>\r\n      <input type=\"text\" v-model=\"wk\" list=\"data-workout\" placeholder=\"Digite o nome exato de um algum dos treinos\" ref=\"addWorkout\">\r\n      <datalist id=\"data-workout\">\r\n        <option v-for=\"wk in workouts\" :key=\"wk.body_part\">{{wk.body_part}}</option>\r\n      </datalist>\r\n      <button type=\"submit\" @click=\"postUserWorkout()\">Adicionar</button>\r\n    </div>\r\n    <div>\r\n      <ul v-for=\"(wk, index) of userWorkouts\" :key=\"index\" class='list-group mr-5 ml-5'>\r\n        <li class=\"list-group-item d-flex justify-content-between align-items-center mt-3\">\r\n          {{ wk }}\r\n          <button class=\"btn btn-danger ml-5 btn-delete pl-3 pr-3 pt-2 pb-2\" @click=\"deleteUserWorkout(index)\">Excluir</button>\r\n        </li>\r\n      </ul>\r\n      <button type=\"submit\" @click=\"sendUserWorkout()\" v-if=\"addUserWorkouts.length >= 3\">Confirmar</button>\r\n    </div>\r\n    <p v-if=\"addUserWorkouts\">\r\n      <ul v-for=\"userWorkout of addUserWorkouts\" :key=\"userWorkout\" id=\"workout-list\">\r\n        <li>{{ userWorkout }}</li>\r\n      </ul>\r\n      <button type=\"submit\" @click=\"sendUserWorkout()\" v-if=\"addUserWorkouts.length === 3\">Confirmar</button>\r\n    </p>\r\n    <p v-else>\r\n      <span>O usuário ainda não tem treinos cadastrados</span>\r\n    </p>\r\n    <br />\r\n    <br />\r\n    <!-- Isso aqui é um componente, que vai ser gerado quando for chamado um botão mostrar treinos -->\r\n    <ul v-for=\"workout in workouts\" :key=\"workout.id\">\r\n      <li>\r\n        <span>Nome: {{ workout.body_part }}</span>\r\n      </li>\r\n      <li>\r\n        <span>Nível: {{ workout.avg_level }}</span>\r\n      </li>\r\n      <ul v-for=\"exercise in workout.exercise\" :key=\"exercise\">\r\n        <li>\r\n          <router-link\r\n            :to=\"{\r\n            name: 'Exercicio', \r\n            params: {id: replaceSpace(exercise), title: exercise},\r\n            }\r\n            \"\r\n            >{{ exercise }}</router-link\r\n          >\r\n        </li>\r\n      </ul>\r\n    </ul>\r\n    <div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport bootstrapvue from \"bootstrap-vue\";\r\nimport axios from \"axios\";\r\nexport default {\r\n  data() {\r\n    return {\r\n      wk: '',\r\n      workouts: [],\r\n      addUserWorkouts: [],\r\n      userWorkouts: sessionStorage.workout.split(\",\"),\r\n    };\r\n  },\r\n  created() {\r\n    this.getWorkouts();\r\n  },\r\n  methods: {\r\n    async getWorkouts() {\r\n        let workout = await axios({\r\n          url: \"http://localhost:8000/workouts/\",\r\n          method: \"GET\",\r\n          headers: {\r\n            Authorization: `Bearer ${sessionStorage.access}`,\r\n          },\r\n        });\r\n        this.workouts = workout.data;\r\n    },\r\n    replaceSpace(word) {\r\n      word = word\r\n        .normalize(\"NFD\")\r\n        .replace(/([\\u0300-\\u036f]|[^0-9a-zA-Z\\s])/g, \"\");\r\n      return word.replaceAll(\" \", \"-\").toLowerCase();\r\n    },\r\n    postUserWorkout() {\r\n      let addWorkout = this.$refs.addWorkout.value;\r\n      this.addUserWorkouts.push(addWorkout);\r\n      addWorkout = \"\";\r\n    },\r\n    deleteUserWorkout(index) {\r\n      this.userWorkouts.splice(index, 1);\r\n    },\r\n    sendUserWorkout() {\r\n      let sendWorkout = {};\r\n      this.addUserWorkouts.forEach((wk, index) => {\r\n        sendWorkout[index.toString()] = wk;\r\n      });\r\n      axios({\r\n        method: \"patch\",\r\n        url: `http://localhost:8000/auths/${sessionStorage.id.toString()}/`,\r\n        data: {\r\n          workout: sendWorkout,\r\n        },\r\n        headers: {\r\n          Authorization: `Bearer ${sessionStorage.access}`,\r\n        },\r\n      })\r\n        .then(() => {\r\n          swal.fire({\r\n            icon: \"success\",\r\n            title: \"Treinos adicionados com sucesso\",\r\n          });\r\n        })\r\n        .catch((e) => {\r\n          swal.fire({\r\n            icon: \"error\",\r\n            title: `Erro: ${e}`,\r\n          });\r\n        });\r\n    },\r\n  },\r\n  computed: {\r\n    originalWorkout() {\r\n      return sessionStorage.workout.split(\",\");\r\n    },\r\n  },\r\n};\r\n</script>"],"sourceRoot":"src/views"}]}