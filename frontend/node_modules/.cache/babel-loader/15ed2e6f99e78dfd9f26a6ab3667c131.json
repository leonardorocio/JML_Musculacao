,gBAAA,GAXA,GAWA,EAXA;;AAYA,qBAAA,EAAA,IAAA,IAAA,EAAA;AACA,sBAAA,EAAA,KAAA,SAAA,EAAA;AACA,oBAAA,OADA,GACA,IAAA,CAAA,EAAA,CADA;;AAEA,yBAAA,IAAA,IAAA,OAAA,EAAA;AACA,sBAAA,GAAA,CAAA,IAAA,CAAA,OAAA,CAAA,IAAA,CAAA;AACA;;AACA,oBAAA,cAAA,CAAA,OAAA,CAAA,EAAA,EAAA,GAAA;AACA,mBANA,MAMA;AACA,oBAAA,cAAA,CAAA,OAAA,CAAA,EAAA,EAAA,IAAA,CAAA,EAAA,CAAA;AACA;AACA;;AACA,gBAAA,IAAA,CACA,IADA,CACA;AACA,kBAAA,IAAA,EAAA,SADA;AAEA,kBAAA,KAAA,EAAA;AAFA,iBADA,EAKA,IALA,CAKA,YAAA;AACA,kBAAA,KAAA,CAAA,OAAA,CAAA,IAAA,CAAA;AAAA,oBAAA,IAAA,EAAA;AAAA,mBAAA;AACA,iBAPA;AAvBA;AAAA;;AAAA;AAAA;AAAA;AAgCA,gBAAA,QAAA,CAAA,WAAA;;AAhCA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAkCA;AAnCA;AAPA,CAAA","sourcesContent":["<template>\r\n  <body id=\"fundoCadastro\">\r\n    <div\r\n      class=\"container bg-white forms d-flex flex-column justify-content-around\"\r\n    >\r\n      <h1 class=\"display-4 text-center\">Login</h1>\r\n\r\n      <form method=\"POST\">\r\n        <div class=\"form-group\">\r\n          <label for=\"userEmail\">Email: </label>\r\n          <input\r\n            type=\"email\"\r\n            name=\"userEmail\"\r\n            id=\"userEmail\"\r\n            v-model=\"email\"\r\n            class=\"form-control\"\r\n            placeholder=\"Ex: leonardof.rocio@gmail.com\"\r\n            aria-describedby=\"helpId\"\r\n          />\r\n        </div>\r\n        <div class=\"form-group\">\r\n          <label for=\"userPass\">Senha: </label>\r\n          <input\r\n            type=\"password\"\r\n            name=\"userPass\"\r\n            v-model=\"pass\"\r\n            id=\"userPass\"\r\n            class=\"form-control\"\r\n            placeholder=\"*********\"\r\n            aria-describedby=\"helpId\"\r\n          />\r\n        </div>\r\n      </form>\r\n      <button\r\n        class=\"btn btn-primary btn-block rounded-pill\"\r\n        type=\"button\"\r\n        @click=\"login()\"\r\n      >\r\n        Login\r\n      </button>\r\n      <p class=\"text-center\">\r\n        Não possui uma conta? Faça\r\n        <router-link :to=\"{ name: 'Cadastro' }\">Cadastro</router-link>\r\n      </p>\r\n    </div>\r\n  </body>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\nimport router from \"vue-router\";\r\n\r\nconst errorLog = require(\"../components/backend_errors.js\");\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      email: \"\",\r\n      pass: \"\",\r\n    };\r\n  },\r\n  methods: {\r\n    async login() {\r\n      try {\r\n        let response = await axios({\r\n          method: \"POST\",\r\n          url: \"http://localhost:8000/auths/login/\",\r\n          data: {\r\n            email: this.email,\r\n            password: this.pass,\r\n          },\r\n        });\r\n        const data = response.data;\r\n        let arr = [];\r\n        for (let dt in data) {\r\n          if (dt === \"workout\") {\r\n            const workout = data[dt];\r\n            for (let item in workout) {\r\n              arr.push(workout[item]);\r\n            }\r\n            sessionStorage.setItem(dt, arr);\r\n          } else {\r\n            sessionStorage.setItem(dt, data[dt]);\r\n          }\r\n        }\r\n        swal\r\n          .fire({\r\n            icon: \"success\",\r\n            title: \"Login realizado com sucesso\",\r\n          })\r\n          .then(() => {\r\n            this.$router.push({ name: \"Home\" });\r\n          });\r\n      } catch (e) {\r\n        errorLog.errorHandle(e);\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style></style>\r\n"],"sourceRoot":"src/views"}]}